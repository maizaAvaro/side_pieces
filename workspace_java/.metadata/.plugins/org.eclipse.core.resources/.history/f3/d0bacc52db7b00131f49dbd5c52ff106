package shit;

public class Test 
{
	
	public Test()
	{
		
		// blank
		
	} // Test Construct
	
	public int Tester(int var)
	{
		
		int value = 0;
		
		value = var;
		
		return value;
		
	} // Tester

	public static void main(String[] args) 
	{
		
		int index = 0;
		
		Test test = new Test();
		
		index = test.Tester(Integer.parseInt(args[0]));
		
		System.out.println("Integer Passed:  " + index);
		
		index = index + 2;
		
		System.out.println("New Value:  " + index);
		

	} // Main

} // Test
